{
  "name": "dust",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "serve:test": "ng serve --host 0.0.0.0 --port 4201 --configuration testing",
    "clean": "touch dist/readme.md && rm -r dist/*",
    "build-sources": "yarn run tailwind:build && ng build --base-href /dust/",
    "build": "yarn run clean && yarn run build-sources",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "doc-test": "playwright test",
    "doc-test:dev": "playwright test --ui",
    "tailwind:build": "tailwindcss -i tailwind.config.css -o src/styles/tailwind-build.scss",
    "tailwind:watch": "tailwindcss -i tailwind.config.css -o src/styles/tailwind-build.scss --watch",
    "dev": "concurrently \"yarn start\" \"yarn run serve:test\" \"yarn run tailwind:watch\" \"yarn run start-proxy\"",
    "coverage": "ng test --no-watch --code-coverage",
    "start-proxy": "node cors_proxy/server.js",
    "sonar:scan": "sonar-scanner -Dsonar.projectKey=Dust -Dsonar.host.url=http://localhost:9000 -Dsonar.token=<sonar-token>"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.1.3",
    "@angular/cdk": "^19.1.1",
    "@angular/common": "^19.1.3",
    "@angular/compiler": "^19.1.3",
    "@angular/core": "^19.1.3",
    "@angular/forms": "^19.1.3",
    "@angular/material": "^18.2.14",
    "@angular/material-luxon-adapter": "^19.1.1",
    "@angular/platform-browser": "^19.1.3",
    "@angular/platform-browser-dynamic": "^19.1.3",
    "@angular/router": "^19.1.3",
    "ace-common-components": "^19.0.1",
    "ace-common-util": "^",
    "immutable": "^4.3.6",
    "luxon": "^3.4.3",
    "rxjs": "~7.5.7",
    "tslib": "^2.7.0",
    "zone.js": "^0.14.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.14",
    "@angular/cli": "^18.2.14",
    "@angular/compiler-cli": "^19.1.3",
    "@endyjasmi/karma-playwright-launcher": "^0.0.1",
    "@playwright/test": "^1.49.1",
    "@tailwindcss/cli": "^4.0.4",
    "@types/express": "^4.17.20",
    "@types/jasmine": "~4.6.4",
    "@types/luxon": "^3.3.7",
    "@types/node": "^20.17.17",
    "concurrently": "^9.0.1",
    "jasmine-core": "~4.6.1",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.1",
    "karma-jasmine": "~4.0.2",
    "karma-jasmine-html-reporter": "~1.7.0",
    "sonar-scanner": "^1.0.1",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.0",
    "typescript": "~5.6.3"
  }
}